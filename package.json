{
  "name": "presentation-react-intro",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "yarn test -- --coverage",
    "serve": "yarn build && serve -p 3000 -s build/",
    "flow": "flow",
    "lint": "eslint src/",
    "css-types": "css-modules-flow-types -e=module.scss src/",
    "flow:watch":
      "nodemon -q --exec \"clear && yarn flow\" --ext js,jsx --watch tst --watch src",
    "css-types:watch": "css-modules-flow-types -w -e=module.scss src/",
    "flow:css-types:watch": "npm-run-all --parallel flow:watch css-types:watch",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize.css": "^7.0.0",
    "preact": "8.2.7",
    "preact-compat": "3.18.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "spectacle": "^4.0.5"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-app": "3.0.2",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "css-modules-flow-types-cli": "1.1.0",
    "css-modules-flow-types-loader": "1.1.0",
    "dotenv": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "0.63.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "node-sass": "4.5.3",
    "nodemon": "1.14.11",
    "npm-run-all": "4.1.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "1.10.2",
    "pretty-quick": "1.3.0",
    "promise": "8.0.1",
    "react-dev-utils": "3.1.0",
    "react-error-overlay": "1.0.10",
    "sass-loader": "6.0.6",
    "serve": "6.4.9",
    "style-loader": "0.18.2",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dashboard": "1.0.0-2",
